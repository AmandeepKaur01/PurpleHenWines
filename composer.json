{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^2",
        "drupal/allowed_formats": "^1",
        "drupal/block_field": "^1",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9",
        "drupal/chosen": "^3",
        "drupal/diff": "^1",
        "drupal/druxt": "^0.4.1",
        "drupal/field_group": "^3",
        "drupal/geofield": "^1",
        "drupal/geofield_map": "^2",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_login": "^1.0@RC",
        "drupal/jsonapi_views": "1.x-dev",
        "drupal/metatag": "^1",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1",
        "drupal/redis": "^1.4",
        "drupal/stage_file_proxy": "^1",
        "drupal/token": "^1",
        "drupal/viewsreference": "^2",
        "drupal/webform": "^5.13",
        "drupal/workbench": "^1.2",
        "drupal/workbench_access": "^1.0@beta",
        "drush/drush": "^10.3",
        "platformsh/config-reader": "^2.3",
        "predis/predis": "^1.1",
        "zaporylie/composer-drupal-optimizations": "^1"
    },
    "require-dev": {
        "drupal/core-dev": "^9",
        "grasmash/drupal-security-warning": "^1.1",
        "previousnext/phpunit-finder": "^1",
        "weitzman/drupal-test-traits": "^1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "excludes": [
                "robots.txt",
                "sites/default/default.services.yml",
                "sites/default/default.settings.php",
                "sites/example.settings.local.php",
                "sites/example.sites.php",
                ".htaccess"
            ]
        },
        "enable-patching": true,
        "installer-types": ["bower-asset", "npm-asset"],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"],
            "web/libraries/{$name}": ["type:drupal-library", "type:bower-asset", "type:npm-asset"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "drupal/core-dev": "^9.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "weitzman/drupal-test-traits": "^1.5"
    }
}
